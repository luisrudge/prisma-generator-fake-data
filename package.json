  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@prisma/generator-helper": "^6.7.0",
    "@prisma/internals": "^6.7.0",
    "decimal.js": "^10.5.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.13",
    "@types/prettier": "^3.0.0",
    "generate-changelog": "^1.8.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "prisma": "^6.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "homepage": "https://github.com/luisrudge/prisma-generator-fake-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/luisrudge/prisma-generator-fake-data.git"
  },
  "author": "Luis Rudge <luis@luisrudge.net>",
  "keywords": [
