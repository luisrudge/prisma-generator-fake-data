{
  "name": "prisma-generator-fake-data",
  "description": "The easiest way to generate mock data based on your Prisma models!",
  "version": "0.14.3",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-fake-data": "dist/bin.js"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "build": "npx tsc",
    "prepack": "bun run build",
